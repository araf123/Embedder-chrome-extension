{
  "manifest_version": 3,
  "name": "Embed Video & PDF",
  "version": "1.0",
  "description": "Easily embed YouTube videos and PDFs into any webpage!",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
